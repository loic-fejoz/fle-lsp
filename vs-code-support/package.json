{
  "name": "fle-lsp-client",
  "displayName": "FLE LSP Client",
  "description": "FLE Language Support",
  "version": "0.0.1",
  "publisher": "loic-fejoz",
  "repository": {
    "type": "git",
    "url": "https://github.com/loic-fejoz/fle-lsp"
  },
  "homepage": "https://github.com/loic-fejoz/fle-lsp/tree/master/vs-code-support",
  "bugs": {
    "url": "https://github.com/loic-fejoz/fle-lsp/issues"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [
    "onLanguage:fastlogentry"
  ],
  "main": "./client.js",
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "contributes": {
    "languages": [
      {
        "id": "fastlogentry",
        "aliases": [
          "Fast Log Entry",
          "FLE"
        ],
        "extensions": [
          ".fle"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fastlogentry",
        "scopeName": "source.fastlogentry",
        "path": "./syntaxes/fastlogentry.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "FLE Original",
        "uiTheme": "vs",
        "path": "./themes/fle-theme-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "fle.convertGeoJsonUI",
        "title": "FLE: Convert to GeoJSON",
        "category": "FLE"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "fle.convertGeoJsonUI",
          "when": "editorLangId == fastlogentry"
        }
      ]
    }
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.0.0"
  }
}