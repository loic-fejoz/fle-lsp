{
  "name": "fle-lsp-client",
  "displayName": "FLE LSP Client",
  "description": "FLE Language Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [
    "onLanguage:fastlogentry"
  ],
  "main": "./client.js",
  "contributes": {
    "languages": [
      {
        "id": "fastlogentry",
        "aliases": [
          "Fast Log Entry",
          "FLE"
        ],
        "extensions": [
          ".fle"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
